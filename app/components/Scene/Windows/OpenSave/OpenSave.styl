.cover-screen{
  background-color: rgba(23, 23, 23, 0.65);
  position:fixed;
  top:0;
  right:0;
  left:0;
  bottom:0;
}

.center-window{
  width: 300px;
  position: absolute;
  left: 50%;
  margin-left: -170px !important
  top: 50%;
  margin-top: -200px;
  height: 400px;
  i{
    position: absolute;
    right: 20px;
    top: 20px;
  }

}

.open-window{
  ul{
    height:250px;
    overflow-y:auto;
  }
  li{
    line-height: 30px;
    border-bottom: 1px solid white;
  }
}
